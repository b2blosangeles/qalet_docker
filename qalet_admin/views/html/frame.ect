<!DOCTYPE html>
<html>
    <% include 'html/htmlHeader.ect' %>
    <body>
        <% include 'html/pageHeader.ect' %>
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <% switch @module : %>
                   
                        <% when 'Home' : %>
                            <% include 'html/home.ect' %>
                        <% end %>
                        
                        <% when 'About' : %>
                            <% include 'html/about.ect' %>
                        <% end %>
 
                         <% when 'dbs' : %>
                           <% include 'html/virtualHosts.ect' %>
                        <% end %>
                        
                        <% when 'virtualHosts' : %>
                            <% include 'html/virtualHosts.ect' %>
                        <% end %>
                        
                        <% when 'addVirtualHosting' : %>
                            <% include 'html/addVirtualHosting.ect' %>
                        <% end %>
                        
                        <% when 'phpMyAdmin' : %>
                            <% include 'html/phpMyAdmin.ect' %>
                        <% end %>
                        
                    <% end %>
                </div>
            </div>
        </div>
        <% include 'html/pageFooter.ect' %>
    </body>
</html>
